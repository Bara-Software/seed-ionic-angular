    "@nrwl/tao": "11.5.1",
    "@nrwl/workspace": "11.5.1",
    "@nstudio/angular": "11.1.1",
    "@nstudio/ionic": "11.1.1",
    "@nstudio/ionic-angular": "11.1.1",
    "@nstudio/web-angular": "11.1.1",
    "@nstudio/xplat": "11.1.1",
    "@types/jest": "~25.1.4",
    "@types/node": "12.12.38",
    "codelyzer": "~6.0.0",
    "cypress": "^6.0.1",
    "dotenv": "6.2.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "prettier": "2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
